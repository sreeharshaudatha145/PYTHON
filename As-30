def print_multiplication_table(m, n):
    for i in range(1, n + 1):
        print(f"{m} x {i} = {m * i}")
# Example usage
m = int(input("Enter the number for which you want the multiplication table: "))
n = int(input("Enter the number up to which you want the multiplication table: "))
print_multiplication_table(m, n)
